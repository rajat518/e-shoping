<%@ page language="java" import="java.sql.*" %>


<%
String driver = "sun.jdbc.odbc.JdbcOdbcDriver";
Class.forName(driver).newInstance();


Connection con=null;


Statement ps=null;


con=DriverManager.getConnection("jdbc:odbc:ibm2","scott","tiger");
ps=con.createStatement();
%>
<%
		String s1=request.getParameter("prodname");
		String s2=request.getParameter("catname");
		String id,name;
		int price,qty,value;
		int i=0;
        %>


<br>
<form action="./Deleteprod" method="post">
<table width="770" border="1">
<caption align="center"><b>INVENTORY LEVEL</b></caption>
<thead><th>Sl.No.</th><th>Product ID</th><th>Product Name</th><th>Price</th><th>Stock</th><th>Stock Value</th><th>Delete</th></thead>
<TBody>
<%  ResultSet  rs=ps.executeQuery("select pro_id,pro_name,pro_price,pro_qty from product where pro_name='"+s1+"' or pro_cat='"+s2+"'");
while(rs.next())
{ 
id=rs.getString(1);
name=rs.getString(2);
price=Integer.parseInt(rs.getString(3));
qty=Integer.parseInt(rs.getString(4));
i++;
%>

<tr>
<td><%=i%>
<td><input type="text" name="id<%=i%>" value=<%=id%>></input></td>

<td><%=name%></td>			
<td><%=	price%></td>
<td><%= qty%></td>			
<% value=price*qty;%>
<td><%=value%></td>	
<td><input type="submit" name="del<%=i%>" value="DELETE<%=i%>"></td>		
</tr>				

<%}

%>
</TBody>
</table><br><input type="hidden" name="x" value="<%=i%>"></form>
